[core]
  repositoryformatversion = 0
  filemode = true
  bare = false
  logallrefupdates = true
  editor = vim
[user]
  name = Srdjan Grubor
  email = sgnn7@sgnn7.org
[color]
  status = auto
  commit = auto
  diff = auto
  branch = auto
[alias]
  st = status
  st = status
  co = checkout
  ci = commit
  br = branch
  ff = merge origin
  diff = diff --ignore-space-change
  dst = diff --staged
[apply]
  whitespace = strip
  whitespace = nowarn
[http]
  sslVerify = 0
  sslverfiy = 0
[push]
  default = simple
[credential]
  helper = cache --timeout=28800
